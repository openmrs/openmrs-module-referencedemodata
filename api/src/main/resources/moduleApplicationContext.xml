<?xml version="1.0" encoding="UTF-8"?>

<!-- Beans to add to the current Application context definition -->

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
  		    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="demoConditionGenerator" class="org.openmrs.module.referencedemodata.condition.DemoConditionGeneratorImpl">
        <property name="conceptService" ref="conceptService" />
        <property name="conditionService" ref="conditionService" />
     </bean>
    <bean id="demoDiagnosisGenerator" class="org.openmrs.module.referencedemodata.diagnosis.DemoDiagnosisGeneratorImpl">
        <property name="conditionGenerator" ref="demoConditionGenerator" />
        <property name="diagnosisService" ref="diagnosisService" />
    </bean>
</beans>
